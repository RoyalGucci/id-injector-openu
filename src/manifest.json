{
	"name": "ID Injector Openu",
	"version": "0.1.0",
	"description": "Automatically fills the ID field in the openu login page",
	"permissions": [
		"storage",
		"tabs"
	],
	"host_permissions": [
		"https://sso.apps.openu.ac.il/*"
	],
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": [
				"https://sso.apps.openu.ac.il/*"
			],
			"js": [
				"contentScript.js"
			],
			"all_frames": true
		}
	],
	"action": {
		"default_title": "ID Injector Openu",
		"default_popup": "popup.html"
	},
	"manifest_version": 3
}